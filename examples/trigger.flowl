// An example of event trigger
load "go:print"
load "go:time"
load "go:sleep"
load "go:event_tick"
load "go:event_cron"

var ev
event {
    // co event_tick -> ev {
    //     "duration": "10s"
    // }
    co event_cron -> ev {
        "expr": "*/5 * * * * *"
    }
}

var t
var s = "hello world!!!"

co sleep {
    "duration" : "10s"
}
co time -> t
co print {
    "_" : "$(t.now) $(ev.which) $(s)"
}