load "go:print"
load "go:sleep"
load "go:time"

var t
var counter = 0

for $(counter) < 10 {
    counter <- $(counter) + 1

    co time -> t
    co print {
        "Time": "$(t.now) / $(t.year) / $(t.month) / $(t.day) $(counter)"
    }
    co sleep

    if $(counter) % 2 == 0 {
        co print {
            "Even Num": "$(counter)"
        }
    }
}
